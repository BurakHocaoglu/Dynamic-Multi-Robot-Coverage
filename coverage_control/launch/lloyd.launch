<?xml version="1.0" encoding="UTF-8"?>

<launch>

	<!-- <arg name="use_mavros" default="false" /> -->
	<arg name="voronoi" default="true" />

	<rosparam command="load" file="$(find coverage_control)/scripts/generic_params.yaml" />

	<!-- Monitor node -->
	<node name="monitor" pkg="coverage_control" type="monitor_node.py" output="screen" />

	<!-- UAS nodes -->
	<node name="uas_0" pkg="coverage_control" type="uas_node.py" args="0 $(arg voronoi)" output="screen" />
	<node name="uas_1" pkg="coverage_control" type="uas_node.py" args="1 $(arg voronoi)" output="screen" />
	<node name="uas_2" pkg="coverage_control" type="uas_node.py" args="2 $(arg voronoi)" output="screen" />
	<node name="uas_3" pkg="coverage_control" type="uas_node.py" args="3 $(arg voronoi)" output="screen" />

</launch>